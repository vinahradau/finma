       KlassifizierungMetadaten'==\{KUNDENNAME, PROMINENZSTATUS\},
       Rollen'==\{(ROLLEGUIBENUTZER, KUNDENNAME),
                  (ROLLEGUIBENUTZER, PROMINENZSTATUS)\},
       RollenRollen'==\{ROLLEGUIBENUTZER\},
